{
  "manifest_version": 3,
  "name": "Responsys Helper",
  "description": "Work simpler",
  "version": "2.0",
  "action": {
    "default_popup": "main_page.html",
    "default_icon": "./images/responsys-logo.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "*://interact.rsys8.net/*",
    "*://v8h1pzy-api.responsys.ocs.oraclecloud.com/*"
  ]
}